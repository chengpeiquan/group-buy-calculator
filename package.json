    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "less": "^4.1.3",
    "prettier": "^2.8.1",
